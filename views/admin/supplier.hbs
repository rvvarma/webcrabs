{{#doc}}
<div class="col-md-offset-5">
<img src="data:image/png;base64,{{this.requestBody.profile.photo}}" class="img-circle" alt="Cinque Terre" width="204" height="136">
</div>
<br>

<div class="modal-body row">
  <div class="col-md-4">
    <!-- Your first column here -->
    <div class="panel panel-primary">

      <div class="panel-heading" style="background-color: #e97319">Supplier Details</p></div>
      <div class="panel-body">
        <p><b>Company Name :</b>  {{this.requestBody.supplier.company_name}} <b>Phone 2   :</b> {{this.requestBody.supplier.phone_2}}</p>
      <p><b>Marks :</b>{{this.requestBody.supplier.marks}}  <b>Pan    :</b> {{this.requestBody.supplier.pan}}</p>
    <p><b>Phone 1:</b>{{this.requestBody.supplier.phone_1}}  <b>GST    :</b> {{this.requestBody.supplier.gst}}</p>

  </div>
    </div>
    </div>



      <div class="col-md-4">
        <!-- Your first column here -->
        <div class="panel panel-primary">

          <div class="panel-heading" style="background-color: #e97319">Payment Terms</p></div>
          <div class="panel-body">
            <p><b>Payment Type :</b>{{this.requestBody.supplier.payterms}} </p>
          <p><b>Bank Name :</b>{{this.requestBody.supplier.bankname}}</p>
         <p><b>Account No :</b>{{this.requestBody.supplier.account_number}} <b> IFSC </b>: {{this.requestBody.supplier.ifsc}}</p>

      </div>
        </div>
        </div>



  <div class="col-md-4">
    <!-- Your first column here -->
    <div class="panel panel-primary">

      <div class="panel-heading" style="background-color: #e97319">Shipment Terms & Rate Card</p></div>
      <div class="panel-body">
        <p><b>Shipment Type :</b>{{this.requestBody.supplier.shipment_type}}</p>
      <p><b>Shipment Days :</b>{{this.requestBody.supplier.shipment_terms}}</p>
    <p><b>Rate Card :</b>{{this.requestBody.supplier.rate_card}}</p>

  </div>
    </div>
    </div>






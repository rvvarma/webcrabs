
<div class="container">
<P>{{mes}}</p>
    <div class="row">
        <div class="panel panel-primary filterable">
            <div class="panel-heading"  style="background-color: #e97319">Details:


                <div class="pull-right">
                    <button class="btn btn-default btn-xs btn-filter"><span class="glyphicon glyphicon-filter"></span> Filter</button>
                </div>
            </div>
            <table class="table">
                <thead>
                <tr class="filters">
                    <th><input type="text" class="form-control" placeholder="Name"></th>

                    <th><input type="text" class="form-control" placeholder="Phone Number"></th>
                    <th><input type="text" class="form-control" placeholder="Gender"></th>
                    <th><input type="text" class="form-control" placeholder="Date Of Birth"></th>
                    <th><input type="text" class="form-control" placeholder="Address"></th>
                     <th><input type="text" class="form-control" placeholder="Completed By"></th>
                    <th><input type="text" class="form-control" placeholder="Farmer"></th>
                    <th><input type="text" class="form-control" placeholder="Supplier"></th>
                    <th><input type="text" class="form-control" placeholder="Buyer"></th>



                </tr>
                </thead>
                <tbody>
                 {{#doc}}
                    <tr>

                      <td>{{this.requestBody.profile.Name}}</td>
                      <td></td>
                      <td>{{this.requestBody.profile.Gender}}</td>
                      <td>{{this.requestBody.profile.DOB}}</td>
                      <td>{{this.requestBody.profile.address}}</td>
                      <td>{{this.requestBody.completed_by}}</td>
                      {{#if this.requestBody.farmer_b}}
                                            <td><a href="/farmer/{{this._id}}">Yes</a></td>
                                            {{else}}
                                              <td><a href="#">No</a></td>
                                            {{/if}}
{{#if this.requestBody.supplier_b}}
                                            <td><a href="/supplier/{{this._id}}">yes</a></td>
                                            {{else}}
                                              <td><a href="#">No</a></td>
                                            {{/if}}
                        {{#if this.requestBody.buyer_b}}
                                                                    <td><a href="/buyer/{{this._id}}">yes</a></td>
                                                                    {{else}}
                                                                      <td><a href="#">No</a></td>
                                                                    {{/if}}

                    </tr>
                    {{/doc}}
                </tbody>
            </table>

        </div>
    </div>
</div>

